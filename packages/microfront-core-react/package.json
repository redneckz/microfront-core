    "build:dev": "webpack --mode=development",
    "test": "jest --coverage"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "dependencies": {
    "@redneckz/microfront-core": "workspace:*"
  },
