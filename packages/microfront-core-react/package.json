{
  "name": "@redneckz/microfront-core-react",
  "version": "0.1.10",
  "description": "Microfrontend lightweight core library for React",
  "keywords": [
    "microfrontend",
    "react"
  ],
  "license": "MIT",
  "author": {
    "name": "redneckz",
    "email": "aalexandrovv@gmail.com"
  },
  "main": "dist/main.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "src",
    "lib",
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
