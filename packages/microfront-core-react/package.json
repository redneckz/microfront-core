    "@types/jest": "26.0.22",
    "@types/node": "14.14.37",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2",
    "@types/react-test-renderer": "^17",
    "jest": "26.6.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-test-renderer": "17.0.2",
    "rimraf": "3.0.2",
    "ts-jest": "26.5.4",
    "ts-loader": "8.1.0",
    "tslib": "2.2.0",
    "typescript": "4.2.3",
    "webpack": "5.36.2",
    "webpack-cli": "4.6.0"
  }
